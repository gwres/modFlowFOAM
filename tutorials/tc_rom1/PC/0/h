FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      h;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //
dimensions      [0 1 0 0 0 0 0];

internalField   uniform 50.0;

boundaryField
{
	specifiedHead
    {
        type            fixedValue;
        value			uniform	50.0;
    }
    wellBoundary
    {
        type            pumpingConfinedBC;
        qC				uniform 0.1842;
        value           uniform 0;
    }
    defaultFaces
    {
        type            empty;
    }
}


