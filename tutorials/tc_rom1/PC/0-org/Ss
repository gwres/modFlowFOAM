FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      Ss;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //
dimensions      [0 -1 0 0 0 0 0];

internalField   uniform 1e-4;

boundaryField
{
	specifiedHead
    {
        type            zeroGradient;
    }
    wellBoundary
    {
        type            zeroGradient;
    }
    defaultFaces
    {
        type            empty;
    }
}

